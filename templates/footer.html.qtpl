{% func Footer(c Postblog) %}
<footer class="footer has-background-dark">
    <div class="content has-text-centered">
        <p class="has-text-grey-lighter">{%s c.Site.Author %} &copy; {%v c.BuildTime.Year() %}</p>
        <p>created with <a class="has-text-grey-dark" href="https://github.com/machineandme/postblog">postblog</a></p>
    </div>
</footer>
{% endfunc %}